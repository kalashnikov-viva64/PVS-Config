					<buildTimeoutSeconds>300</buildTimeoutSeconds>
					<successExitCodes>0</successExitCodes>
				</exec>
				<exec>
					<executable>C:\Windows\System32\cmd.exe</executable>
					<description>Analyze x86</description>
